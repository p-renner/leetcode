{
	"compilerOptions": {
		"noImplicitAny": true,
		"noEmit": true,
		"baseUrl": ".",
		"moduleResolution": "node",
		"paths": {
			"*": [
				"node_modules/*"
			],
			"@/*": [
				"src/*"
			]
		},
		"target": "ES2020",
		"useDefineForClassFields": true,
		"lib": [
			"ES2020",
			"DOM",
			"DOM.Iterable"
		],
		"module": "ESNext",
		"isolatedModules": true
	}
}
